{
    "openapi": "3.0.3",
    "info": {
        "title": "Spokane Economic Dashboard",
        "description": "Local API for handling interactions between the MongoDB cluster and the Census Bureau API",
        "version": "0.3.2",
        "contact": {
            "email": "zfoteff@protonmail.com",
            "name": "Zac Foteff"
        }
    },
    "tags": [{
            "name": "pn",
            "description": "Page Navigation"
        },
        {
            "name": "lfp",
            "description": "Labor Force Participation Rate"
        },
        {
            "name": "mhi",
            "description": "Median Household Income"
        },
        {
            "name": "hai",
            "description": "Housing affordability Index"
        },
        {
            "name": "mhr",
            "description": "Median Home Resale Value"
        }
    ],
    "servers": [{
        "url": "https://spokaneeconomicdashboard.herokuapp.com/v1",
        "description": "Production server"
    }],
    "paths": {
        "/": {
            "get": {
                "tags": ["pn"],
                "summary": "Index page Navigation Route",
                "description": "Returns a HTML page that renders the home screen on the user's machine",
                "operationId": "getHomePage",
                "responses": {
                    "200": {
                        "description": "getHomePage response",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/newLabor": {
            "post": {
                "tags": ["lfp"],
                "summary": "Create new labor indicator data point endpoint",
                "description": "Endpoint facilitates interactions with MongoDB to create new indicator data for the Labor Force Participation Rate key indicator",
                "operationId": "createLabor",
                "responses": {}
            }
        }
    }
}